#include <iostream>
using namespace std;

int main() {
	// your code goes here
	for (int i = 0; i < 6; i++)
    {
        int val = 1;
        
        for (int k = 0; k <= i; k++)
        {
            cout << " " << val;
            val = val * (i - k) / (k + 1);
        }
        cout << endl << endl;
    }
    cout << endl;
	return 0;
}
